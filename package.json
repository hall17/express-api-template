  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon",
    "build": "cross-env NODE_ENV=production swc src -d dist --source-maps --copy-files",
    "start-prod": "cross-env NODE_ENV=production node dist/server.js",
    "build-tsc": "tsc  && tsc-alias",
    "prisma-generate": "prisma generate",
    "prisma-seed": "dotenv -e .env.development -- yarn ts-node src/prisma/seed.ts",
    "prisma-migrate": "dotenv -e .env.development -- prisma migrate dev",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint-check": "yarn tsc --noEmit && yarn eslint . --ext .ts,.js,.json ",
    "fix-all": "yarn format && yarn lint"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
